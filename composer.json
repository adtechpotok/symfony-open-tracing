{
  "name": "adtechpotok/symfony-open-tracing",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Roman Minatulaev",
      "email": "r.minatulaev@rambler-co.ru"
    }
  ],
  "require": {
    "php": ">=7.1",
    "jonahgeorge/jaeger-client-php": "~0.4",
    "symfony/config": "~3.4|~4.0",
    "symfony/console": "~3.4|~4.0",
    "symfony/dependency-injection": "~3.4.6|~4.1",
    "symfony/http-kernel": "~3.4|~4.1",
    "symfony/serializer-pack": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4@dev"
  },
  "autoload": {
    "psr-4": {
      "Adtechpotok\\Bundle\\SymfonyOpenTracing\\": "src/",
      "Adtechpotok\\Bundle\\SymfonyOpenTracing\\Tests\\": "Tests/"
    }
  },
  "config": {
    "vendor-dir": "vendor",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "^7.4@dev"
  }
}
